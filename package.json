{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "setup": "sh \"scripts/setup.sh\"",
        "db:fresh": "php artisan migrate:fresh && php artisan db:seed DevSeeder",
        "deploy:staging": "git checkout staging && git merge origin/dev && git push && git checkout dev"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.5",
        "axios": "^0.25",
        "bootstrap": "^5.1.3",
        "bootstrap-icons": "^1.8.1",
        "jquery": "^3.6.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "tiny-slider": "^2.9.4"
    }
}
